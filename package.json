{
  "name": "rn-starter",
  "main": "./index.ts",
  "version": "1.0.1",
  "workspaces": [
    "src/*"
  ],
  "scripts": {
    "start": "expo start --dev-client",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "lint": "eslint './**/*.{ts,tsx}'",
    "lint:fix": "eslint './**/*.{ts,tsx}' --fix",
    "type-check": "tsc --pretty --noEmit",
    "build": "dotenv -- npx eas-cli@latest build --local --output devClient/ios.tar.gz --profile development-simulator --platform ios --clear-cache",
    "build:prod:ios": "dotenv -- npx eas-cli@12.5.4 build --local --output prod/ios.ipa --profile production --platform ios --clear-cache",
    "build:prod:android": "npx eas-cli@12.5.4 build --local --output prod/android.aab --profile production --platform android --clear-cache",
    "build:dev:android": "npx eas-cli@12.5.4 build --local --output devClient/android.apk --profile development-simulator --platform android",
    "build:eas:android": "eas build --platform android --profile production",
    "build:eas:ios": "eas build --platform ios --profile production",
    "generate-navigation": "ts-node -T scripts/generate-navigation.ts",
    "postinstall": "patch-package"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "./src/**": [
    "prettier --write ."
  ],
  "dependencies": {
    "@expo/react-native-action-sheet": "4.1.0",
    "@expo/vector-icons": "^14.0.2",
    "@gorhom/portal": "1.0.14",
    "@grapp/stacks": "3.1.0",
    "@hookform/resolvers": "3.9.0",
    "@mobily/ts-belt": "3.13.1",
    "@react-native-community/checkbox": "0.5.17",
    "@react-native-picker/picker": "2.7.5",
    "@react-navigation/bottom-tabs": "6.6.1",
    "@react-navigation/drawer": "6.7.2",
    "@react-navigation/native": "6.1.18",
    "@react-navigation/native-stack": "6.11.0",
    "@react-navigation/stack": "6.4.1",
    "@tanstack/react-query": "5.51.23",
    "@trivago/prettier-plugin-sort-imports": "4.3.0",
    "axios": "1.7.6",
    "colord": "2.9.3",
    "dotenv": "16.4.7",
    "expo": "51.0.15",
    "expo-checkbox": "3.0.0",
    "expo-constants": "~16.0.2",
    "expo-dev-client": "~4.0.29",
