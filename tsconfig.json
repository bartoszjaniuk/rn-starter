{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"strict": true,
		"baseUrl": ".",
		"strictNullChecks": true,
		"noUncheckedIndexedAccess": true
	},
	"include": [
		"src/**/*.ts",
		"assets/**/*otf.",
		"src/**/*.tsx",
		"index.ts",
		"**/*.ts",
		"**/*.tsx",
		".expo/types/**/*.ts",
		"expo-env.d.ts"
	],
	"exclude": ["node_modules", "dist", "ios", "android", "scripts", "app.config.ts", "editorWeb"]
	
}
